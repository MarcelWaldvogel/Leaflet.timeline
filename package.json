{
  "name": "leaflet.timeline",
  "version": "1.4.2",
  "description": "Show GeoJSON objects on a timeline",
  "scripts": {
    "prebuild": "if [ -d dist ]; then rm -rf dist; fi",
    "build": "tsc && webpack",
    "postbuild": "cp dist/leaflet.timeline.js docs/examples/leaflet.timeline.js",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/skeate/Leaflet.timeline.git"
  },
  "keywords": [
    "leaflet",
    "timeline",
    "playback",
    "time",
    "animate"
  ],
  "author": "Jonathan Skeate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skeate/Leaflet.timeline/issues"
  },
  "homepage": "https://github.com/skeate/Leaflet.timeline",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/leaflet": "^1.5.11",
    "@types/simulant": "^0.2.0",
    "@yaga/generic-geojson": "^1.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "jest": "^25.1.0",
    "leaflet": "^1.6.0",
    "simulant": "^0.2.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "leaflet": "^1.2.0"
  },
  "dependencies": {
    "diesal": "^1.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "testMatch": [
      "**/test/*_test.ts"
    ]
  },
  "files": [
    "dist/*"
  ]
}
